<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.fms.eis.webapp.dao.PCalcTplDAO">
    <select id="findByKey" parameterType="PCalcTplDomain"
            resultType="PCalcTplDomain" useCache="true">
        select ID,NAME,TYPE,DEFAULT_FLAG,CREATOR_ID,CREATE_DATE,LAST_MODIFIER_ID,LAST_MODIFY_TIME
        from P_CALC_TPL
        <where>
            and ID = #{id}
        </where>
    </select>
    <select id="findByWhere" parameterType="PCalcTplDomain"
            resultType="PCalcTplDomain" useCache="true">
        select ID,NAME,TYPE,DEFAULT_FLAG,CREATOR_ID,CREATE_DATE,LAST_MODIFIER_ID,LAST_MODIFY_TIME
        from P_CALC_TPL
        <where>
            <if test="id !=null">
                and ID = #{id}
            </if>
            <if test="name !=null">
                and NAME = #{name}
            </if>
            <if test="type !=null">
                and TYPE = #{type}
            </if>
            <if test="defaultFlag !=null">
                and DEFAULT_FLAG = #{defaultFlag}
            </if>
            <if test="creatorId !=null">
                and CREATOR_ID = #{creatorId}
            </if>
            <if test="createDate !=null">
                and CREATE_DATE = #{createDate}
            </if>
            <if test="lastModifierId !=null">
                and LAST_MODIFIER_ID = #{lastModifierId}
            </if>
            <if test="lastModifyTime !=null">
                and LAST_MODIFY_TIME = #{lastModifyTime}
            </if>
        </where>
    </select>
    <insert id="insert" parameterType="PCalcTplDomain" flushCache="true" useGeneratedKeys="true">
		insert into P_CALC_TPL
			(ID,NAME,TYPE,DEFAULT_FLAG,CREATOR_ID,CREATE_DATE,LAST_MODIFIER_ID,LAST_MODIFY_TIME)
			VALUES(#{id},#{name},#{type},#{defaultFlag},#{creatorId},#{createDate},#{lastModifierId},#{lastModifyTime})

	</insert>
    <update id="update" parameterType="PCalcTplDomain" flushCache="true">
        update P_CALC_TPL
        <set>
            <if test="name !=null">
                NAME = #{name},
            </if>
            <if test="type !=null">
                TYPE = #{type},
            </if>
            <if test="defaultFlag !=null">
                DEFAULT_FLAG = #{defaultFlag},
            </if>
            <if test="creatorId !=null">
                CREATOR_ID = #{creatorId},
            </if>
            <if test="createDate !=null">
                CREATE_DATE = #{createDate},
            </if>
            <if test="lastModifierId !=null">
                LAST_MODIFIER_ID = #{lastModifierId},
            </if>
            <if test="lastModifyTime !=null">
                LAST_MODIFY_TIME = #{lastModifyTime},
            </if>
        </set>
        <where>
            and ID = #{id}
        </where>
    </update>
    <delete id="delete" parameterType="PCalcTplDomain" flushCache="true">
        delete from P_CALC_TPL
        <where>
            and ID = #{id}
        </where>
    </delete>
</mapper>