<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.fms.eis.webapp.dao.PTaskDetailDAO">
    <select id="findByKey" parameterType="PTaskDetailDomain"
            resultType="PTaskDetailDomain" useCache="true">
        select ID,TPL_DETAIL_ID,INFOPOINT_TYPE,DATA_ID,WEIGHT,CREATOR_ID,CREATE_DATE,LAST_MODIFIER_ID,LAST_MODIFY_TIME
        from P_TASK_DETAIL
        <where>
            and ID = #{id}
        </where>
    </select>
    <select id="findByWhere" parameterType="PTaskDetailDomain"
            resultType="PTaskDetailDomain" useCache="true">
        select ID,TPL_DETAIL_ID,INFOPOINT_TYPE,DATA_ID,WEIGHT,CREATOR_ID,CREATE_DATE,LAST_MODIFIER_ID,LAST_MODIFY_TIME
        from P_TASK_DETAIL
        <where>
            <if test="id !=null">
                and ID = #{id}
            </if>
            <if test="tplDetailId !=null">
                and TPL_DETAIL_ID = #{tplDetailId}
            </if>
            <if test="infopointType !=null">
                and INFOPOINT_TYPE = #{infopointType}
            </if>
            <if test="dataId !=null">
                and DATA_ID = #{dataId}
            </if>
            <if test="weight !=null">
                and WEIGHT = #{weight}
            </if>
            <if test="creatorId !=null">
                and CREATOR_ID = #{creatorId}
            </if>
            <if test="createDate !=null">
                and CREATE_DATE = #{createDate}
            </if>
            <if test="lastModifierId !=null">
                and LAST_MODIFIER_ID = #{lastModifierId}
            </if>
            <if test="lastModifyTime !=null">
                and LAST_MODIFY_TIME = #{lastModifyTime}
            </if>
        </where>
    </select>
    <insert id="insert" parameterType="PTaskDetailDomain" flushCache="true" useGeneratedKeys="true">
		INSERT INTO P_TASK_DETAIL
			(ID,TPL_DETAIL_ID,INFOPOINT_TYPE,DATA_ID,WEIGHT,CREATOR_ID,CREATE_DATE,LAST_MODIFIER_ID,LAST_MODIFY_TIME)
			VALUES(#{id},#{tplDetailId},#{infopointType},#{dataId},#{weight},#{creatorId},#{createDate},#{lastModifierId},#{lastModifyTime})

	</insert>
    <update id="update" parameterType="PTaskDetailDomain" flushCache="true">
        update P_TASK_DETAIL
        <set>
            <if test="id !=null">
                ID = #{id},
            </if>
        </set>
        <set>
            <if test="tplDetailId !=null">
                TPL_DETAIL_ID = #{tplDetailId},
            </if>
        </set>
        <set>
            <if test="infopointType !=null">
                INFOPOINT_TYPE = #{infopointType},
            </if>
        </set>
        <set>
            <if test="dataId !=null">
                DATA_ID = #{dataId},
            </if>
        </set>
        <set>
            <if test="weight !=null">
                WEIGHT = #{weight},
            </if>
        </set>
        <set>
            <if test="creatorId !=null">
                CREATOR_ID = #{creatorId},
            </if>
        </set>
        <set>
            <if test="createDate !=null">
                CREATE_DATE = #{createDate},
            </if>
        </set>
        <set>
            <if test="lastModifierId !=null">
                LAST_MODIFIER_ID = #{lastModifierId},
            </if>
        </set>
        <set>
            <if test="lastModifyTime !=null">
                LAST_MODIFY_TIME = #{lastModifyTime},
            </if>
        </set>
        <where>
            and ID = #{ID}

        </where>
    </update>
    <delete id="delete" parameterType="PTaskDetailDomain" flushCache="true">
        delete from P_TASK_DETAIL
        <where>
            and ID = #{id}

        </where>
    </delete>
</mapper>